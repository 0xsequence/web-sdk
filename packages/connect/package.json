{
  "name": "@0xsequence/connect",
  "version": "5.0.4",
  "description": "Connect package for Sequence Web SDK",
  "keywords": [
    "sequence",
    "web-sdk",
    "wallet"
  ],
  "repository": "https://github.com/0xsequence/web-sdk/tree/master/packages/connect",
  "author": "Horizon Blockchain Games",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run writeVersion && pnpm build:clean && pnpm build:css && pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc --module commonjs --declaration --declarationMap --outDir dist/cjs && echo '{ \"type\": \"commonjs\" }' > dist/cjs/package.json",
    "build:esm": "tsc --module es2022 --declaration --declarationMap --outDir dist/esm",
    "build:css": "rimraf ./src/styles.ts && tailwindcss -i ./src/index.css -o - | echo \"export const styles = String.raw\\`\n$(cat)\\`\" > ./src/styles.ts",
    "build:clean": "rimraf -g ./dist",
    "build:check": "attw --pack .",
    "dev": "pnpm run writeVersion && pnpm pnpm build:css && tsc --watch --module es2022 --declaration --declarationMap --outDir dist/esm",
    "writeVersion": "node scripts/writeVersion.js",
    "test": "echo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@0xsequence/api": ">= 2.2.13",
    "@0xsequence/auth": ">= 2.2.13",
    "@0xsequence/core": ">= 2.2.13",
    "@0xsequence/design-system": "^2.0.11",
    "@0xsequence/ethauth": "^1.0.0",
    "@0xsequence/hooks": "workspace:*",
    "@0xsequence/indexer": ">= 2.2.13",
    "@0xsequence/metadata": ">= 2.2.13",
    "@0xsequence/network": ">= 2.2.13",
    "@0xsequence/provider": ">= 2.2.13",
    "@0xsequence/utils": ">= 2.2.13",
    "@0xsequence/waas": ">= 2.2.13",
    "@tailwindcss/cli": "^4.0.14",
    "clsx": "^2.1.1",
    "fuse.js": "^6.6.2",
    "motion": "^12.3.1",
    "tailwindcss": "^4.0.6",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "0xsequence": "^2.2.13",
    "@databeat/tracker": "^0.9.3",
    "@react-oauth/google": "^0.11.1",
    "@tanstack/react-query": ">= 5",
    "ethers": ">=6.13.0",
    "react": ">= 17",
    "react-apple-signin-auth": "^1.1.0",
    "react-dom": ">= 17",
    "viem": ">= 2.0.0",
    "wagmi": ">= 2.14.13"
  },
  "devDependencies": {
    "0xsequence": "^2.2.13",
    "@tanstack/react-query": "^5.62.0",
    "@types/uuid": "^9.0.8",
    "ethers": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.23.10",
    "wagmi": "^2.14.13"
  }
}
